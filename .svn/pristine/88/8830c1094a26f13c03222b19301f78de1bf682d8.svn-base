<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core">

    <h:head>
        <title>Login</title>
    </h:head>
    <h:body>
        <h:outputLabel value="Occupation - SuppportComm" style="font-size: 18"/>
    
	    <h:form>
	        <p:panel  header="Do Login for begin" >
	            <h:panelGrid columns="2" >
	            Email : <h:inputText   id="email" value="#{loginBean.email}"/>
	            Senha : <h:inputSecret id="senha" value="#{loginBean.password}"/>
	            </h:panelGrid>
	            <p:commandButton id="login" style="font-size: smaller" value="access" action="#{loginBean.login}"/>
	            
	        </p:panel>
	    </h:form>
        
        </h:body>

</html>

