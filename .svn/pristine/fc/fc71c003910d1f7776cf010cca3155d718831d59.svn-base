<html xmlns="http://www.w3.org/1999/xhtml"
     xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
     xmlns:p="http://primefaces.org/ui"
     xmlns:c="http://java.sun.com/jsp/jstl/core">
 <h:head><title>Occupation</title></h:head>
 <h:body>
<h:form id="form">

    
	
	

		    <h:panelGrid columns="1" >
				<p:poll interval="3" update="ocpPie" />
	
			    <p:pieChart id="ocpPie" value="#{chartBean.livePieOccupationTotalByLogin}" title="Total Occupation" showDataLabels="true"
			                legendPosition="nw" style="width:500px;height:300px" >
			                  
			                
			     </p:pieChart>
		     
			     
			     <p:commandButton value="Show Chart by Portal" 
                                 action="#{chartBean.showDetailByPortal}" ajax="false"/>
                                            
			</h:panelGrid>

		

	
</h:form>
</h:body>
</html> 